{
  "name": "milieux-blocks",
  "version": "1.0.1",
  "private": true,
  "license": "GPL-3.0-or-later",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js"
  },
  "dependencies": {
    "@wordpress/api-fetch": "^3.14.0",
    "autoprefixer": "^9.7.6",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-cgb": "^1.7.0",
    "cgb-dev-utils": "^1.6.0",
    "chalk": "^4.0.0",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.2",
    "cross-spawn": "^7.0.2",
    "date-fns": "^2.13.0",
    "eslint": "^6.8.0",
    "eslint-config-wordpress": "^2.0.0",
    "eslint-plugin-jest": "^23.9.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-wordpress": "^0.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "filesize": "^6.1.0",
    "fs-extra": "^9.0.0",
    "gzip-size": "^5.1.1",
    "inquirer": "^7.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.25.3",
    "node-sass": "^7.0.0",
    "ora": "^4.0.4",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^4.0.1",
    "react-reorder": "^3.0.0-alpha.7",
    "resolve-pkg": "^2.0.0",
    "sass-loader": "^8.0.2",
    "shelljs": "^0.8.4",
    "style-loader": "^1.2.1",
    "update-notifier": "^4.1.0",
    "webpack": "^4.43.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false,
          "targets": {
            "browsers": [
              "last 2 Chrome versions",
              "last 2 Firefox versions",
              "last 2 Safari versions",
              "last 2 iOS versions",
              "last 1 Android version",
              "last 1 ChromeAndroid version"
            ]
          }
        }
      ]
    ],
    "plugins": [
      [
        "transform-object-rest-spread"
      ],
      [
        "transform-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ],
      [
        "transform-react-jsx",
        {
          "pragma": "wp.element.createElement"
        }
      ],
      [
        "transform-runtime",
        {
          "helpers": false,
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  },
  "devDependencies": {
    "webpack-livereload-plugin": "^2.3.0"
  }
}
